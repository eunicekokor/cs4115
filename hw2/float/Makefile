TARFILES = Makefile scanner.mll parser.mly float.ml

OBJS = parser.cmo scanner.cmo float.cmo

float : $(OBJS)
	ocamlc -o float $(OBJS)

scanner.ml : scanner.mll
	ocamllex scanner.mll

parser.ml parser.mli : parser.mly
	ocamlyacc parser.mly

%.cmo : %.ml
	ocamlc -c $<

%.cmi : %.mli
	ocamlc -c $<

.PHONY : clean
clean :
	rm -f float parser.ml parser.mli scanner.ml *.cmo *.cmi

# Generated by ocamldep *.ml *.mli
float.cmo: scanner.cmo parser.cmi
float.cmx: scanner.cmx parser.cmx
parser.cmo: parser.cmi 
parser.cmx: parser.cmi 
scanner.cmo: parser.cmi 
scanner.cmx: parser.cmx 
parser.cmi: 
